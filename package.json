{
  "name": "qwittig-backend",
  "version": "0.0.1",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "DEBUG=* VERBOSE=1 babel-node src/server.js",
    "build": "rm -rf build && mkdir build && babel src -d build",
    "production": "pm2 startOrRestart process.json --env production",
    "start": "node build/server.js",
    "deploy:netcup": "bash deploy.sh",
    "deploy:google": "npm run build && gcloud app deploy --project qwittig-6fb93"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "debug": "latest",
    "express": "^4.14.0",
    "firebase": "^3.4.0",
    "firebase-queue": "^1.5.0",
    "fraction.js": "^3.3.1",
    "google-cloud": "^0.40.0",
    "lodash": "^4.16.1",
    "moment": "^2.15.1",
    "moment-range": "^2.2.0",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "mz": "^2.4.0",
    "request": "^2.75.0",
    "request-promise": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-2": "^6.13.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "nodemon": "^1.10.2"
  }
